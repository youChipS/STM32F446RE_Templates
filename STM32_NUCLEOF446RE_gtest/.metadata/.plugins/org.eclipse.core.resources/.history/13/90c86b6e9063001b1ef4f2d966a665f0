#include "myMath.h"
#include "gtest_helper.h"

/******************************************************************************/
/* Google Test C Helpers */
/******************************************************************************/
void myMath_add(void)
{
	ExpectEqual(0, add(0,0));
	ExpectEqual(1, add(0,1));
	ExpectEqual(1, add(1,0));
	ExpectEqual(2, add(1,1));
}

